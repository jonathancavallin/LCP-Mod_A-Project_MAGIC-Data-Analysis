   "source": [
    "X_rescaled = X.copy()\n",
    "original_alpha = X['fAlpha'].copy()\n",
    "\n",
    "for i in range(X.shape[1]):\n",
    "    h = X[str(X.columns[i])]\n",
    "    avg = np.average(h)\n",
    "    std = np.std(h)\n",
    "    h = h - avg\n",
    "    h = h / std\n",
    "    X_rescaled[str(X_rescaled.columns[i])] = h\n",
    "    \n",
    "print('media length:',np.average(X_rescaled['fLength']))\n",
    "print('dev_std length:',np.std(X_rescaled['fLength']))"
   ]
