    "cm = confusion_matrix(Y_test,Y_pred)\n",
    "log_cm = normalize_cm(cm)\n",
    "\n",
    "print('Confusion matrix:')\n",
    "print(log_cm)\n",
    "plot_cm(log_cm,method_name='Logistic Regression')\n"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "cf5010ba",
   "metadata": {},
   "source": [
    "Here we plot the best model ROC curve"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "2fbd84a7",
   "metadata": {},
   "outputs": [],
   "source": [
    "fpr_log = fpr_test\n",
    "tpr_log = tpr_test\n",
    "\n",
    "plt.figure()\n",
    "plt.plot(fpr_test, tpr_test, color='#d173b8', lw=4, label=f'ROC curve (Partial AUC = {partial_auc_test:.3f}, AUC = {auc_test:.3f})')\n",
    "plt.plot([0, 1], [0, 1], color='red', linestyle='--') \n",
    "plt.xlim([0.0, 1.0])\n",
    "plt.ylim([0.0, 1.02])\n",
    "plt.xlabel('False Positive Rate',fontsize=13.8)\n",
    "plt.ylabel('True Positive Rate',fontsize=13.8)\n",
    "plt.title('Logistic Regression ROC Curve',fontsize=14.3)\n",
