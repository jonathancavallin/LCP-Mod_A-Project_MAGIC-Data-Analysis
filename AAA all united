    "ax[1].set_yticks(ticks, minor=False)\n",
    "ax[1].set_xticklabels(features, rotation=90)\n",
    "ax[1].set_yticklabels(features)\n",
    "ax[1].grid(which='minor', color='black', linewidth=1)\n",
